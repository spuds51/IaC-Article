{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "./serilog.txt"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "Xerris.Hungrr.Api"
    }
  },
  "SecretConfigurations": {
    "Items":[
      {
        "Name": "Xerris",
        "SecretId": "xerris/hungrr",
        "Region": "us-west-2"
      }
    ]
  },
  "DbInfo": {
    "Host": "localhost",
    "Port": "26257",
    "UserName": "maxroach",
    "Database": "hungrr",
    "Password": "hungrr"
  }
}